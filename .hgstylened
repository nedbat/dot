# Style file for hg log output.
changeset = "{label('custom.rev', rev)}{label('custom.rev', ':')}{label('custom.rev', short(node))} {label('custom.when', date|shortdate)} {ifeq(branch,'default','',label('custom.branch','{branch} '))}{if(bookmarks,label('custom.branch','{bookmarks} '))}{if(tags,label('custom.branch','{tags} '))}{desc|firstline} {label('custom.author', author|person)}\n"
